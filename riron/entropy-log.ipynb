   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "P0 = np.arange(0,1, 0.01) \n",
    "P1 = 1 - P0 \n",
    "\n",
    "# エントロピー\n",
    "H = -P0 * np.log2(P0) -P1 * np.log2(P1)\n",
    "\n",
    "# グラフ描画設定\n",
    "plt.plot(P0, H)\n",
    "plt.xlabel(\"P0\")\n",
    "plt.ylabel(\"H\")\n",
    "plt.title('Entropy')\n",
    "\n",
    "# ラベルの描画\n",
    "plt.legend()\n",
    "# グラフの描画実行\n",
    "plt.show()"
   ]
